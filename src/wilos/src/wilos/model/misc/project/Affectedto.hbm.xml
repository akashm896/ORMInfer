<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>

	<class name="wilos.model.misc.project.Affectedto" table="affectedto" lazy="true">
		<composite-id>
	 		<key-property name="project_id" column="project_id" type="string"></key-property>
	 		<key-property name="participant_id" column="participant_id" type="string"/>
	 	</composite-id>
	</class>
</hibernate-mapping>